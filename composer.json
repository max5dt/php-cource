{
    "name": "mkrasikov/php-cource",
    "autoload": {
        "psr-4": {
            "MyApp\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "squizlabs/php_codesniffer": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5"
    },
    "scripts": {
        "lint": "phpcs --standard=PSR12 ./src",
        "linttests": "phpcs --standard=PSR12 ./tests",
        "test": "phpunit"
    }
}
